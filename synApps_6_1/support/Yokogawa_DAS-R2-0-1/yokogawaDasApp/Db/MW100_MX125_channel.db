record(bo, "$(P)$(DAU):$(ADDRESS):RelaySet")
{
    field(DTYP, "Yokogawa MW100")
    field(OUT,  "@$(HANDLE) VAL:$(ADDRESS)")
    field(ZNAM, "Open")
    field(ONAM, "Close")
}
record(stringout,"$(P)$(DAU):$(ADDRESS):Label")
{
    field(PINI, "YES")
    field( OUT, "$(P)$(DAU):$(ADDRESS):Relay.DESC PP")
}

