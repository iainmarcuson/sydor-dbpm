record(luascript, "$(P)$(Q)state_machine")
{	
	field(INAA, "$(P)$(Q)aaval.VAL")
	field(INBB, "$(P)$(Q)bbval.VAL")
	field(INCC, "$(P)$(Q)ccval.VAL")
	field(INDD, "$(P)$(Q)ddval.VAL")
	field(INEE, "$(P)$(Q)eeval.VAL")
	field(INFF, "$(P)$(Q)ffval.VAL")
	field(INGG, "$(P)$(Q)ggval.VAL")
	field(INHH, "$(P)$(Q)hhval.VAL")
	field(INII, "$(P)$(Q)iival.VAL")
	field(INJJ, "$(P)$(Q)jjval.VAL")
	
	field(PREC, 1)
	
	field(PINI, 1)
	
	field(CODE, "@examples.lua state_demo()")
}

